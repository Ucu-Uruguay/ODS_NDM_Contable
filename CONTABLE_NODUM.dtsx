    ct_dptos.cod_clasedpto, ct_dptos.cod_dpto,ct_contratoscamp.nro_trans_aux trplan,
ct_contratoscamp.activo_fcc cto_activo,ct_valcuotascont.activo_fcc tr_activo,ct_numeracuotas.nro_cuota, 
cast(convert(varchar(6),dateadd(month,nro_cuota-1,convert(date,cast(ct_valcuotascont.aniomesdesde*100+1 AS varchar(8)),112)),112) AS int) aniomes,
row_number() over(partition BY ct_contratoscamp.num_cont
ORDER BY cast(convert(varchar(6),dateadd(month,nro_cuota-1,convert(date,cast(ct_valcuotascont.aniomesdesde*100+1 AS varchar(8)),112)),112) AS int) ASC)  AS numeral_cuota,
isnull(cong.valorActual,ct_valcuotascont.valorActual) AS valor_actual_cuota_b,  ct_contratoscamp.cod_moneda,
dateadd(month,nro_cuota-1,convert(date,cast(ct_valcuotascont.aniomesdesde*100+ct_valcuotAScont.dia_cobro AS varchar(8)),112)) AS fec_venc,
CASE WHEN ct_valcuotascont.tipo_tramo='B' THEN 'CONTRATO' ELSE CASE WHEN ct_valcuotascont.tipo_tramo='a' THEN 'RECURSE' ELSE 'ACTIVIDAD' END END   tipo_cuota, 
cast(ct_valcuotAScont.idfccproceso AS varchar(20)) idfccproceso, ct_concepfaccont.cod_concepto

FROM ct_valcuotascont 
LEFT OUTER JOIN ct_licenciaacade licencia
ON licencia.num_cont=ct_valcuotascont.num_cont AND licencia.cod_emp=ct_valcuotascont.cod_emp 
AND licencia.aniomesdesde&gt;=ct_valcuotascont.aniomesdesde AND licencia.aniomeshasta&lt;=ct_valcuotascont.aniomeshasta
INNER JOIN ct_numeracuotas ON ct_numeracuotas.nro_cuota&lt;=(ct_valcuotascont.cant_cuotas + ISnull(licencia.cant_meses,0))   
INNER JOIN ct_concepfaccont ON ct_concepfaccont.idfccconcepto=ct_valcuotascont.idfccconcepto 
INNER JOIN ct_contratoscamp ON ct_valcuotascont.num_cont=ct_contratoscamp.num_cont
                        INNER JOIN ct_dptos ON ct_dptos.cod_dpto=ct_contratoscamp.cod_dpto
